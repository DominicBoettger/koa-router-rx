{
  "name": "koa-router-rx",
  "version": "0.1.0",
  "description": "Reactive router middleware for koa",
  "main": "dist/index.js",
  "repository": "https://github.com/mfellner/koa-router-rx.git",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "license": "MIT",
  "keywords": [
    "route",
    "router",
    "middleware",
    "koa",
    "Observable",
    "Observables",
    "ReactiveExtensions",
    "ReactiveX",
    "RxJS",
    "Rx"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dist": "babel -d dist lib",
    "start": "babel-node example/server.js",
    "test": "NODE_ENV=test jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "flow-typed-install": "flow-typed install"
  },
  "dependencies": {
    "koa-router": "7.0.1",
    "rxjs": "5.0.0-rc.4"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-jest": "17.0.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-preset-latest-minimal": "1.1.2",
    "flow-bin": "0.36.0",
    "flow-typed": "2.0.0",
    "jest": "17.0.3",
    "koa": "2.0.0-alpha.7",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
